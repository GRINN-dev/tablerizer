{
  "schemas": ["$SCHEMAS"],
  "out": "${OUTPUT_DIR:./exports}",
  "roles": ["$ADMIN_ROLE", "$USER_ROLE", "${VISITOR_ROLE:visitor}"],
  "database_url": "$DATABASE_URL",
  "scope": "${EXPORT_SCOPE:all}",
  "role_mappings": {
    "$ADMIN_ROLE": ":DATABASE_ADMIN",
    "$USER_ROLE": ":DATABASE_USER",
    "$VISITOR_ROLE": ":DATABASE_VISITOR"
  }
}